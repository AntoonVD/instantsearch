{
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "plugins": [
        "prettier",
        "html"
    ],
    "rules": {
        "prettier/prettier": ["error", {"trailingComma": "es5", "singleQuote": true, "bracketSpacing": true}]
    }
}

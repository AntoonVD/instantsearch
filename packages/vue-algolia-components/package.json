{
  "name": "vue-algolia-components",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "dist/vue-algolia-components.common.js",
  "module": "dist/vue-algolia-components.esm.js",
  "jsnext:main": "dist/vue-algolia-components.esm.js",
  "unpkg": "dist/vue-algolia-components.js",
  "scripts": {
    "build": "yarn build-umd && yarn build-es && yarn build-cjs",

    "build-cjs": "rollup -c ../../build/rollup.cjs.config.js"
  },
  "dependencies": {
    "vue-algolia-search-store": "*",
    "vue-algolia-search-input": "*",
    "vue-algolia-search-results": "*",
    "vue-algolia-search-stats": "*",
    "vue-algolia-ranged-pagination": "*",
    "vue-algolia-results-per-page-selector": "*",
    "vue-algolia-navigation-tree-facet": "*",
    "vue-algolia-sort-by-selector": "*",
    "vue-algolia-clear-search": "*",
    "vue-algolia-stars-facet": "*",
    "vue-algolia-empty-results": "*",
    "vue-algolia-search-facet": "*",
    "vue-algolia-price-range-facet": "*"
  },
  "peerDependencies": {
    "vue": "^2.2.2"
  }
}
